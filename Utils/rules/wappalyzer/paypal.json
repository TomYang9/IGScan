{
    "name": "PayPal",
    "website": "https://paypal.com",
    "matches": [
        {
            "regexp": "<input[^>]+_s-xclick"
        },
        {
            "regexp": "<img [^>]*src=\"[^/]*//[^/]*paypal\\.com"
        },
        {
            "regexp": "<img [^>]*src=\"[^/]*//[^/]*paypalobjects\\.com"
        },
        {
            "search": "script",
            "regexp": "paypalobjects\\.com"
        }
    ]
}